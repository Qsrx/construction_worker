<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:gravity="center_vertical"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/item_dowork_small_number"
        android:layout_margin="@dimen/dp_10"
        android:layout_width="31dp"
        android:layout_height="31dp"
        android:background="@drawable/shape_unselect"
        android:gravity="center"
        android:text="A"
        android:textColor="@color/color_c3"/>
    <TextView
        android:id="@+id/item_dowork_small_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="工期和费用索赔均能成立"
        android:textColor="@color/color_c6"/>
</LinearLayout>
